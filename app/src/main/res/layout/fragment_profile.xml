<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".profile" android:id="@+id/frameLayout"
                                                   android:background="@color/screenBackground">
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="Aquino-Ellul"
            android:ems="10"
            android:id="@+id/editTextTextPersonName"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/textColors"
            android:layout_marginEnd="12dp" android:layout_marginRight="12dp"
            app:layout_constraintStart_toEndOf="@+id/lastname" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2" android:layout_marginTop="4dp"/>
    <TextView
            android:text="Profile Information"
            android:layout_width="109dp"
            android:layout_height="49dp" android:id="@+id/textView8"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:textColor="@color/textColors" android:textSize="36dp" android:layout_marginTop="44dp"/>
    <TextView
            android:text="First Name: "
            android:layout_width="134dp"
            android:layout_height="35dp" android:id="@+id/textView9"
            app:layout_constraintStart_toStartOf="parent"
            android:textSize="25dp" android:textColor="@color/textColors" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8" android:layout_marginTop="32dp"/>
    <EditText
            android:layout_width="211dp"
            android:layout_height="41dp"
            android:inputType="textPersonName"
            android:text="Paulinho"
            android:ems="10"
            android:id="@+id/editTextTextPersonName2"
            app:layout_constraintStart_toEndOf="@+id/textView9" app:layout_constraintEnd_toEndOf="parent"
            android:textColor="@color/textColors"
            app:layout_constraintTop_toBottomOf="@+id/textView8" android:layout_marginTop="32dp"
            app:layout_constraintHorizontal_bias="0.02"/>
    <TextView
            android:text="Last Name: "
            android:layout_width="133dp"
            android:layout_height="39dp" android:id="@+id/lastname"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:textColor="@color/textColors"
            android:textSize="25dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9" android:layout_marginTop="16dp"/>
    <Button
            android:text="Statistics "
            android:background="@drawable/roundbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/statistics"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="60dp" android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <Button
            android:text="Update"
            android:background="@drawable/roundbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button7"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView8" app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>